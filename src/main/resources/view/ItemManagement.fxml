<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="656.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ItemManagementController">
   <children>
      <Label layoutX="151.0" layoutY="14.0" text="Item Management">
         <font>
            <Font name="Courier New Bold" size="38.0" />
         </font>
      </Label>
      <TableView fx:id="tblitemmanagement" layoutX="110.0" layoutY="77.0" prefHeight="200.0" prefWidth="437.0">
         <columns>
            <TableColumn fx:id="colcode" prefWidth="90.0" text="ItemCode" />
            <TableColumn fx:id="coldescription" prefWidth="90.0" text="Description" />
            <TableColumn fx:id="colpacksize" text="PackSize" />
            <TableColumn fx:id="colunitprice" prefWidth="100.0" text="UnitPrice" />
            <TableColumn fx:id="colqtyonhand" prefWidth="90.0" text="QtyOnHand" />
         </columns>
      </TableView>
      <JFXButton fx:id="btnAddItem" layoutX="31.0" layoutY="316.0" onAction="#btnAddItemOnAction" prefHeight="50.0" prefWidth="139.0" text="ADD">
         <font>
            <Font name="Courier New Bold" size="27.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnUpdateItem" layoutX="183.0" layoutY="316.0" onAction="#btnUpdateItemOnAction" prefHeight="50.0" prefWidth="139.0" text="UPDATE">
         <font>
            <Font name="Courier New Bold" size="27.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnDeleteItem" layoutX="328.0" layoutY="316.0" onAction="#btnDeleteItemOnAction" prefHeight="50.0" prefWidth="139.0" text="DELETE">
         <font>
            <Font name="Courier New Bold" size="27.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnViewItem" layoutX="472.0" layoutY="316.0" onAction="#btnViewItemOnAction" prefHeight="50.0" prefWidth="139.0" text="VIEW">
         <font>
            <Font name="Courier New Bold" size="27.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
